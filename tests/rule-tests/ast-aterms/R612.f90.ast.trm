        [ AssignmentStmt(
            no-label()
          , Substring(
              DataRef([PartRef("a", no-section-subscripts(), no-image-selector())])
            , SubstringRange(IntLiteralConstant("1", no-kind()), IntLiteralConstant("3", no-kind()))
            )
          , IntLiteralConstant("3", no-kind())
