Program(
  "!! R924 endfile-stmt\n!    is  ENDFILE file-unit-number\n!    or  ENDFILE ( position-spec-list )\n!\n! Not tested here: file-unit-number and position-spec-list.\n"
, [ MainProgram(
      no-program-stmt()
    , initial-spec-part([], [])
    , SpecAndExecPart(
        [ endfile-unit-stmt(no-label(), IntLiteralConstant("10", no-kind()), eos("\n"))
        , EndfileStmt(
            no-label()
          , [ UNIT(IntLiteralConstant("10", no-kind()))
            , IOSTAT(DataRef([PartRef("n", no-section-subscripts(), no-image-selector())]))
            , ERR("4")
            ]
          , eos("\n\n")
          )
        ]
      )
    , no-subprogram-part()
    , EndProgramStmt(no-label(), no-program-name(), eos("\n\n"))
    )
  ]
)
