Program(
  "!! R910 read-stmt\n!    is  READ (io-control-spec-list ) [ input-item-list ]\n!    or  READ format [, input-item-list ]\n!\n! Not tested here: io-control-spec-list, input-item-list, and format\n"
, [ MainProgram(
      no-program-stmt()
    , initial-spec-part([], [])
    , SpecAndExecPart(
        [ ReadStmt(
            no-label()
          , io-control-spec-list(UNIT(IntLiteralConstant("10", no-kind())), no-list())
          , []
          , eos("\n")
          )
        , ReadStmt(
            no-label()
          , io-control-spec-list(UNIT(IntLiteralConstant("10", no-kind())), FMT(STAR()), no-list())
          , [ DataRef([PartRef("a", no-section-subscripts(), no-image-selector())])
            , DataRef([PartRef("b", no-section-subscripts(), no-image-selector())])
            ]
          , eos("\n\n")
          )
        , ReadFMTStmt(
            no-label()
          , IntLiteralConstant("5", no-kind())
          , comma-list(
              [ DataRef([PartRef("a", no-section-subscripts(), no-image-selector())])
              , DataRef([PartRef("b", no-section-subscripts(), no-image-selector())])
              , DataRef([PartRef("c", no-section-subscripts(), no-image-selector())])
              ]
            )
          , eos("\n\n")
          )
        , FormatStmt(
            "5"
          , FormatSpec(
              format-items(
                format-items-first(
                  E(
                    IntLiteralConstant("2", no-kind())
                  , IntLiteralConstant("16", no-kind())
                  , IntLiteralConstant("3", no-kind())
                  , no-e()
                  )
                )
              , I(no-r(), IntLiteralConstant("5", no-kind()), no-m())
              )
            , no-list()
            )
          , eos("\n\n")
          )
        ]
      )
    , no-subprogram-part()
    , EndProgramStmt(no-label(), no-program-name(), eos("\n\n"))
    )
  ]
)
