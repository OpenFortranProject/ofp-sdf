        [ DimensionStmt(
            no-label()
          , [ ArrayNameSpec("X", [ExplicitShape(no-lower-bound(), IntVal("3"))])
            , ArrayNameSpec(
                "Y"
              , [ExplicitShape(no-lower-bound(), IntVal("3")), ExplicitShape(no-lower-bound(), IntVal("2"))]
              )
            ]
          , eos("\n")
          )
        , AssignmentStmt(
            no-label()
          , DataRef([PartRef("X", no-section-subscripts(), no-image-selector())])
