                , DataRef([PartRef("x", no-section-subscripts(), no-image-selector())])
                , IntVal("1")
                , eos("\n")
                )
              ]
            )
          , EndAssociateStmt(no-label(), no-name(), eos("\n\n"))
          )
        , AssociateConstruct(
            AssociateStmt(
              no-label()
