          , [EntityDecl("p", no-list(), no-list(), no-char-length(), no-init())]
          , eos("\n\n!! NOTE 4.59\n!\n")
          )
        , TypeDeclarationStmt(
            "15"
          , derived-type-spec(DerivedTypeSpec("point", no-list()))
          , no-list()
          , [ EntityDecl(
                "PV"
              , no-list()
              , no-list()
              , no-char-length()
