        , AssignmentStmt(
            no-label()
          , DataRef([PartRef("p", no-section-subscripts(), no-image-selector())])
          , DataRef(
              [ PartRef(
                  "PERSON"
                , paren-list(
                    [ActualArgSpec("NAME", "'JOHN SMITH'"), ActualArgSpec("AGE", IntVal("21"))]
                  )
                , no-image-selector()
                )
              ]
            )
          , eos("\n\n")
          )
        ]
      )
    , no-subprogram-part()
    , EndProgramStmt(no-label(), no-program-name(), eos("\n"))
    )
  ]
)
