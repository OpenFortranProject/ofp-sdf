  Level3Expr  '<'   Level3Expr             -> Level4Expr      {cons("LT")}
  Level3Expr  '<='  Level3Expr             -> Level4Expr      {cons("LE")}
  Level3Expr  '>'   Level3Expr             -> Level4Expr      {cons("GT")}
